<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>mystudy</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  	<servlet>
  		<servlet-name>deleteServ</servlet-name>
  		<servlet-class>com.mystudy.pd.serv.deleteServlet</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
  		<servlet-name>deleteServ</servlet-name>
  		<url-pattern>/delServ</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
  		<servlet-name>detailSv</servlet-name>
  		<servlet-class> com.mystudy.pd.serv.detailServlet</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
  		<servlet-name>detailSv</servlet-name>
  		<url-pattern>/detailServ</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
  		<servlet-name>editOkServ</servlet-name>
  		<servlet-class> com.mystudy.pd.serv.EditOkServlet</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
  		<servlet-name>editOkServ</servlet-name>
  		<url-pattern>/editOk</url-pattern>
  	</servlet-mapping>
  	
  	<servlet>
  		<servlet-name>editServ</servlet-name>
  		<servlet-class> com.mystudy.pd.serv.EditServlet</servlet-class>
  	</servlet>
  	
  	<servlet-mapping>
  		<servlet-name>editServ</servlet-name>
  		<url-pattern>/editServlet</url-pattern>
  	</servlet-mapping>
	
</web-app>


